{
  "name": "react-webxr",
  "version": "0.1.0-beta.0",
  "description": "React implementation of the web xr api",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "rm -rf ./lib && tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "prettier": "npm run prettier \"src/**/*.+(ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --noEmit",
    "storybook": "start-storybook -s gltf --port 3000",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisdobby/react-webxr.git"
  },
  "author": "ChrisDobby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisdobby/react-webxr/issues"
  },
  "homepage": "https://react-webxr.chrisdobby.dev",
  "keywords": [
    "react",
    "webxr",
    "augmented reality"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "6.2.2",
    "@storybook/addon-docs": "6.2.2",
    "@storybook/addon-info": "5.3.21",
    "@storybook/react": "6.2.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.0.6",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^25.2.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
