{
  "name": "react-webxr",
  "version": "0.1.0-beta.0",
  "description": "React implementation of the web xr api",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "rm -rf ./lib && tsc",
    "refresh": "tsc",
    "refresh:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prettier": "npm run prettier \"src/**/*.+(ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --noEmit",
    "storybook": "start-storybook -s gltf --port 3000",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisdobby/react-webxr.git"
  },
  "author": "ChrisDobby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisdobby/react-webxr/issues"
  },
  "homepage": "https://react-webxr.chrisdobby.dev",
  "keywords": [
    "react",
    "webxr",
    "augmented reality"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "5.3.13",
    "@storybook/addon-docs": "5.3.13",
    "@storybook/addon-info": "5.3.13",
    "@storybook/react": "5.3.13",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "tslint-react-a11y": "^1.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
