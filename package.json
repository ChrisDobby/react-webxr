{
  "name": "react-webxr",
  "version": "0.0.1",
  "description": "React implementation of the web xr api",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "rm -rf ./lib && tsc",
    "refresh": "tsc",
    "refresh:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prettier": "npm run prettier \"src/**/*.+(ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --noEmit",
    "develop": "yarn refresh && yarn link && cd demos && yarn && yarn link react-webxr && cd .. && npm-run-all --parallel start-demos refresh:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisdobby/react-webxr.git"
  },
  "author": "ChrisDobby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisdobby/react-webxr/issues"
  },
  "homepage": "https://github.com/chrisdobby/react-webxr#readme",
  "devDependencies": {
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "tslint-react-a11y": "^1.1.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5"
  }
}